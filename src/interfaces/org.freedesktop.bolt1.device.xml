<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
                      "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.freedesktop.bolt1.Device">
    <method name="Authorize">
      <arg type="s" name="flags" direction="in">
      </arg>
    </method>
    <property type="s" name="Uid" access="read">
    </property>
    <property type="s" name="Name" access="read">
    </property>
    <property type="s" name="Vendor" access="read">
    </property>
    <property type="s" name="Type" access="read">
    </property>
    <property type="s" name="Status" access="read">
    </property>
    <property type="s" name="AuthFlags" access="read">
    </property>
    <property type="s" name="Parent" access="read">
      <annotation name="org.qtproject.QtDBus.PropertyGetter" value="parentUid"/>
    </property>
    <property type="s" name="SysfsPath" access="read">
    </property>
    <property type="b" name="Stored" access="read">
    </property>
    <property type="s" name="Policy" access="read">
    </property>
    <property type="s" name="Key" access="read">
    </property>
    <property type="s" name="Label" access="readwrite">
    </property>
    <property type="t" name="ConnectTime" access="read">
    </property>
    <property type="t" name="AuthorizeTime" access="read">
    </property>
    <property type="t" name="StoreTime" access="read">
    </property>
  </interface>
</node>
