#: ui/DeviceList.qml:25
#, kde-format
msgid "Enable Thunderbolt devices"
msgstr "Thunderbolt apparaten inschakelen"

#: ui/DeviceList.qml:60
#, kde-format
msgid "No Thunderbolt devices connected"
msgstr "Geen Thunderbolt apparaten verbonden"

#: ui/DeviceList.qml:61
#, kde-format
msgid "Plug in a Thunderbolt device"
msgstr "En Thunderbolt apparaat inpluggen"

#: ui/DeviceView.qml:62
#, kde-format
msgid "Vendor:"
msgstr "Fabrikant:"

#: ui/DeviceView.qml:66
#, kde-format
msgid "UID:"
msgstr "Gebruikers-id:"

#: ui/DeviceView.qml:70
#, kde-format
msgid "Status:"
msgstr "Status:"

#: ui/DeviceView.qml:75
#, kde-format
msgid "Authorized at:"
msgstr "Geautoriseerd op:"

#: ui/DeviceView.qml:80
#, kde-format
msgid "Connected at:"
msgstr "Verbonden op:"

#: ui/DeviceView.qml:85
#, kde-format
msgid "Enrolled at:"
msgstr "ingepast op:"

#: ui/DeviceView.qml:89
#, kde-format
msgid "Yes"
msgstr "Ja"

#: ui/DeviceView.qml:89
#, kde-format
msgid "No"
msgstr "Nee"

#: ui/DeviceView.qml:90
#, kde-format
msgid "Trusted:"
msgstr "Vertrouwd:"

#: ui/DeviceView.qml:99
#, kde-format
msgid "Authorizing..."
msgstr "Wordt geautoriseerd..."

#: ui/DeviceView.qml:99
#, kde-format
msgid "Authorize"
msgstr "Autoriseren"

#: ui/DeviceView.qml:110 ui/DeviceView.qml:130
#, kde-format
msgid "Failed to enroll device <b>%1</b>: %2"
msgstr "Inpassen van apparaat <b>%1</b> is mislukt: %2"

#: ui/DeviceView.qml:117
#, kde-format
msgid "Trust this Device"
msgstr "Dit apparaat vertrouwen"

#: ui/DeviceView.qml:138
#, kde-format
msgid "Revoke Trust"
msgstr "Vertrouwen intrekken"

#: ui/DeviceView.qml:150
#, kde-format
msgid "Error changing device trust: <b>%1</b>: %2"
msgstr "Fout bij het wijzigen van vertrouwen in apparaat: <b>%1</b>: %2"

#: ui/DeviceView.qml:167
#, kde-format
msgid ""
"Hint: trusted device will be automatically authorized the next time it is "
"connected to the computer."
msgstr ""
"Tip: vertrouwd apparaat zal automatisch geautoriseerd worden bij de volgende "
"keer dat het verbonden wordt met de computer."

#: ui/DeviceView.qml:168
#, kde-format
msgid ""
"Hint: an untrusted device needs to be manually authorized each time it is "
"connected to the computer."
msgstr ""
"Tip: een niet vertrouwd apparaat moet handmatig worden geautoriseerd elke "
"keer dat het verbonden wordt met de computer."

#: ui/main.qml:33
#, kde-format
msgid "Thunderbolt support has been disabled in BIOS"
msgstr "Ondersteuning van Thunderbolt is uitgeschakeld in BIOS"

#: ui/main.qml:34
#, kde-format
msgid "Follow your system manufacturer's guide to enable Thunderbolt support"
msgstr ""
"De gids van de producent van uw systeem volgen om ondersteuning voor "
"Thunderbolt in te schakelen"

#: ui/main.qml:42
#, kde-format
msgid "Thunderbolt subsystem is disabled or unavailable"
msgstr "Subsysteem Thunderbolt is uitgeschakeld of niet beschikbaar"

#: ui/main.qml:43
#, kde-format
msgid ""
"If the device supports Thunderbolt, try plugging in a Thunderbolt device"
msgstr ""
"Als het apparaat Thunderbolt ondersteunt, probeer een Thunderbolt apparaat "
"in te pluggen"

