#: ui/DeviceView.qml:112 ui/DeviceView.qml:132
#, kde-format
msgid "Failed to enroll device <b>%1</b>: %2"
msgstr "Ha fallat en inscriure el dispositiu <b>%1</b>: %2"

#: ui/DeviceView.qml:119
#, kde-format
msgid "Trust this Device"
msgstr "Confia en aquest dispositiu"

#: ui/DeviceView.qml:140
#, kde-format
msgid "Revoke Trust"
msgstr "Revoca la confiança"

#: ui/DeviceView.qml:152
#, kde-format
msgid "Error changing device trust: <b>%1</b>: %2"
msgstr "Error en canviar la confiança del dispositiu: <b>%1</b>: %2"

#: ui/DeviceView.qml:169
#, kde-format
msgid ""
"Hint: trusted device will be automatically authorized the next time it is "
"connected to the computer."
msgstr ""
"Suggeriment: el dispositiu de confiança s'autoritzarà automàticament la "
"pròxima vegada que es connecti a l'ordinador."

#: ui/DeviceView.qml:170
#, kde-format
msgid ""
"Hint: an untrusted device needs to be manually authorized each time it is "
"connected to the computer."
msgstr ""
"Suggeriment: el dispositiu sense confiança necessitarà que s'autoritzi "
"manualment cada vegada que es connecti a l'ordinador."

#: ui/main.qml:39
#, kde-format
msgid "Thunderbolt support has been disabled in BIOS"
msgstr "El funcionament del Thunderbolt està desactivat a la BIOS"

#: ui/main.qml:44
#, kde-format
msgid "Thunderbolt subsystem is not available"
msgstr "El subsistema Thunderbolt no està disponible"

