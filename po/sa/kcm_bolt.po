msgid "Disconnected"
msgstr "विच्छिन्नः"

#: ui/utils.js:21
msgid "Connecting"
msgstr "संयोजयन्"

#: ui/utils.js:24
msgid "Connected"
msgstr "संबद्धः"

#: ui/utils.js:28
msgid "Authorization Error"
msgstr "प्राधिकरण त्रुटि"

#: ui/utils.js:31
msgid "Authorizing"
msgstr "अधिकृतीकरण"

#: ui/utils.js:36
msgid "Reduced Functionality"
msgstr "कार्यक्षमता न्यूनीकृता"

#: ui/utils.js:38
msgid "Connected & Authorized"
msgstr "संबद्ध एवं अधिकृत"

#: ui/utils.js:46
msgid "Trusted"
msgstr "विश्वस्तः"
