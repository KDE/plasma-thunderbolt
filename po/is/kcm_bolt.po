msgstr "Auðkenni:"

#: ui/DeviceView.qml:71
#, kde-format
msgid "Status:"
msgstr "Staða:"

#: ui/DeviceView.qml:76
#, kde-format
msgid "Authorized at:"
msgstr "Auðkennt:"

#: ui/DeviceView.qml:81
#, kde-format
msgid "Connected at:"
msgstr "Tengt:"

#: ui/DeviceView.qml:86
#, kde-format
msgid "Enrolled at:"
msgstr "Skráð:"

