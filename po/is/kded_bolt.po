#: kded_bolt.cpp:67
#, kde-format
msgid "New Thunderbolt Device Detected"
msgstr "Nýtt Thunderbolt-tæki fannst"

#: kded_bolt.cpp:68
#, kde-format
msgid ""
"Unauthorized Thunderbolt device <b>%1</b> was detected. Do you want to "
"authorize it?"
msgstr "Thunderbolt-tækið <b>%1</b> án heimildar fannst.  Viltu auðkenna það?"

#: kded_bolt.cpp:70
#, kde-format
msgid ""
"%1 unauthorized Thunderbolt device was detected. Do you want to authorize it?"
msgid_plural ""
"%1 unauthorized Thunderbolt devices were detected. Do you want to authorize "
"them?"
msgstr[0] "%1 Thunderbolt-tæki án heimildar fannst. Viltu auðkenna það?"
msgstr[1] "%1 Thunderbolt-tæki án heimildar fundust.  Viltu auðkenna þau?"

#: kded_bolt.cpp:78
#, kde-format
msgid "Authorize Now"
msgstr "Auðkenna núna"

#: kded_bolt.cpp:83
#, kde-format
msgid "Authorize Permanently"
msgstr "Auðkenna varanlega"

#: kded_bolt.cpp:131
#, kde-format
msgid "Thunderbolt Device Authorization Error"
msgstr "Auðkenningarvilla fyrir Thunderbolt-tæki"

#: kded_bolt.cpp:132
#, kde-format
msgid "Failed to authorize Thunderbolt device <b>%1</b>: %2"
msgstr "Mistókst að auðkenna Thunderbolt-tæki <b>%1</b>: %2"
