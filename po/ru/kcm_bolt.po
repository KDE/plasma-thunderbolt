msgstr "Авторизация"

#: ui/utils.js:29
msgid "Reduced Functionality"
msgstr "Ограниченная функциональность"

#: ui/utils.js:31
msgid "Connected & Authorized"
msgstr "Подключено и авторизовано"

#: ui/utils.js:38
msgid "Trusted"
msgstr "Доверенное"

