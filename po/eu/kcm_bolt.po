#: ui/DeviceView.qml:81
#, kde-format
msgid "Connected at:"
msgstr "Konektatuta hemen:"

#: ui/DeviceView.qml:86
#, kde-format
msgid "Enrolled at:"
msgstr "Erroldatua hemen:"

