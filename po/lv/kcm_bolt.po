#: ui/DeviceList.qml:25
#, kde-format
msgid "Enable Thunderbolt devices"
msgstr "Ieslēgt „Thunderbolt“ ierīces"

#: ui/DeviceList.qml:60
#, kde-format
msgid "No Thunderbolt devices connected"
msgstr "Nav pievienotu „Thunderbolt“ ierīču"

#: ui/DeviceList.qml:61
#, kde-format
msgid "Plug in a Thunderbolt device"
msgstr "Pievienojiet „Thunderbolt“ ierīci"

#: ui/DeviceView.qml:63
#, kde-format
msgid "Vendor:"
msgstr "Ražotājs:"

#: ui/DeviceView.qml:67
#, kde-format
msgid "UID:"
msgstr "UID:"

#: ui/DeviceView.qml:71
#, kde-format
msgid "Status:"
msgstr "Statuss:"

#: ui/DeviceView.qml:76
#, kde-format
msgid "Authorized at:"
msgstr "Autorizēta pie:"

#: ui/DeviceView.qml:81
#, kde-format
msgid "Connected at:"
msgstr "Pievienota pie:"

#: ui/DeviceView.qml:86
#, kde-format
msgid "Enrolled at:"
msgstr "Reģistrēta pie:"

#: ui/DeviceView.qml:90
#, kde-format
msgid "Yes"
msgstr "Jā"

#: ui/DeviceView.qml:90
#, kde-format
msgid "No"
msgstr "Nē"

#: ui/DeviceView.qml:91
#, kde-format
msgid "Trusted:"
msgstr "Uzticama:"

#: ui/DeviceView.qml:100
#, kde-format
msgid "Authorizing..."
msgstr "Autorizē..."

#: ui/DeviceView.qml:100
#, kde-format
msgid "Authorize"
msgstr "Autorizēt"

#: ui/DeviceView.qml:111 ui/DeviceView.qml:131
#, kde-format
msgid "Failed to enroll device <b>%1</b>: %2"
msgstr "Neizdevās reģistrēt ierīci <b>%1</b>: %2"

#: ui/DeviceView.qml:118
#, kde-format
msgid "Trust this Device"
msgstr "Uzticēties šai ierīcei"

#: ui/DeviceView.qml:139
#, kde-format
msgid "Revoke Trust"
msgstr "Atsaukt uzticēšanos"

#: ui/DeviceView.qml:151
#, kde-format
msgid "Error changing device trust: <b>%1</b>: %2"
msgstr "Kļūda, mainot ierīces uzticamību: <b>%1</b>: %2"

#: ui/DeviceView.qml:168
#, kde-format
msgid ""
"Hint: trusted device will be automatically authorized the next time it is "
"connected to the computer."
msgstr ""
"Padoms: uzticamu ierīci sistēma automātiski autorizēs pēc tās nākamās "
"pievienošanas datoram."

#: ui/DeviceView.qml:169
#, kde-format
msgid ""
"Hint: an untrusted device needs to be manually authorized each time it is "
"connected to the computer."
msgstr ""
"Padoms: neuzticamu ierīci vajadzēs manuāli autorizēt katru reizi, kad to "
"pievienosiet datoram."

#: ui/main.qml:33
#, kde-format
msgid "Thunderbolt support has been disabled in BIOS"
msgstr "„Thunderbolt“ atbalsts ir izslēgts BIOS līmenī"

#: ui/main.qml:34
#, kde-format
msgid "Follow your system manufacturer's guide to enable Thunderbolt support"
msgstr "Lai ieslēgtu „Thunderbolt“ atbalstu, sekojiet ražotāja instrukcijām"

#: ui/main.qml:42
#, kde-format
msgid "Thunderbolt subsystem is disabled or unavailable"
msgstr "„Thunderbolt“ apakšsistēma nav ieslēgta vai nav pieejama"

#: ui/main.qml:43
#, kde-format
msgid ""
"If the device supports Thunderbolt, try plugging in a Thunderbolt device"
msgstr ""
"Ja ierīce atbalsta „Thunderbolt“, mēģiniet pievienot „Thunderbolt“ ierīci"

