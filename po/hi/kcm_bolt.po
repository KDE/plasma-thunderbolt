msgid "Authorized at:"
msgstr "प्राधिकृत:"

#: ui/DeviceView.qml:74
#, kde-format
msgid "Connected at:"
msgstr "यहां से जुड़े:"

#: ui/DeviceView.qml:79
#, kde-format
msgid "Enrolled at:"
msgstr "नामांकित स्थान:"

#: ui/DeviceView.qml:83
#, kde-format
msgid "Yes"
msgstr "हाँ"

