#: ui/DeviceView.qml:112
#, kde-format
msgid "Trust this Device"
msgstr "Confiar neste dispositivo"

#: ui/DeviceView.qml:133
#, kde-format
msgid "Revoke Trust"
msgstr "Revogar confiança"

#: ui/DeviceView.qml:145
