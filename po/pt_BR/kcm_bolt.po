#, kde-format
msgid "Authorize"
msgstr "Autorizar"

#: ui/DeviceView.qml:111 ui/DeviceView.qml:131
#, kde-format
msgid "Failed to enroll device <b>%1</b>: %2"
msgstr "Não foi possível registrar o dispositivo <b>%1</b>: %2"

#: ui/DeviceView.qml:118
#, kde-format
msgid "Trust this Device"
msgstr "Confiar neste dispositivo"

#: ui/DeviceView.qml:139
#, kde-format
msgid "Revoke Trust"
msgstr "Revogar confiança"

#: ui/DeviceView.qml:151
#, kde-format
msgid "Error changing device trust: <b>%1</b>: %2"
msgstr "Erro ao modificar a confiança no dispositivo: <b>%1</b>: %2"

#: ui/DeviceView.qml:168
#, kde-format
msgid ""
"Hint: trusted device will be automatically authorized the next time it is "
"connected to the computer."
msgstr ""
"Dica: um dispositivo confiável será autorizado automaticamente da próxima "
"vez que for conectado ao computador."

#: ui/DeviceView.qml:169
#, kde-format
msgid ""
"Hint: an untrusted device needs to be manually authorized each time it is "
"connected to the computer."
msgstr ""
"Dica: um dispositivo não confiável precisa ser autorizado manualmente a cada "
"vez que for conectado ao computador."

#: ui/main.qml:37
#, kde-format
msgid "Thunderbolt support has been disabled in BIOS"
msgstr "O suporte para Thunderbolt foi desativado na BIOS"

#: ui/main.qml:42
#, kde-format
msgid "Thunderbolt subsystem is not available"
msgstr "O subsistema Thunderbolt não está disponível"

