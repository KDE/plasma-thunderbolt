msgstr "Habilitar"

#: ui/main.qml:32
#, kde-format
msgctxt "@option:check"
msgid "Enable Thunderbolt subsystem"
msgstr "Habilita subsistema Thunderbolt"

#: ui/main.qml:63
#, kde-format
msgid "Thunderbolt support has been disabled in BIOS"
msgstr "O suporte para Thunderbolt foi desativado na BIOS"

#: ui/main.qml:64
#, kde-format
msgid "Follow your system manufacturer's guide to enable Thunderbolt support"
msgstr ""
"Siga o guia do fabricante do seu sistema para ativar o suporte ao Thunderbolt"

#: ui/main.qml:70
#, kde-format
msgid "No Thunderbolt devices connected"
msgstr "Nenhum dispositivo Thunderbolt conectado"

#: ui/main.qml:71
#, kde-format
