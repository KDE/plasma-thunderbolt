#: kded_bolt.cpp:67
#, kde-format
msgid "New Thunderbolt Device Detected"
msgstr "Yeni Thunderbolt cihazı aşkar edildi"

#: kded_bolt.cpp:68
#, kde-format
msgid ""
"Unauthorized Thunderbolt device <b>%1</b> was detected. Do you want to "
"authorize it?"
msgstr ""
"İcazəsiz <b>%1</b>Thunderbolt cihazı aşkar edildi. Buna icazə vermək "
"istəyirsiniz?"

#: kded_bolt.cpp:70
#, kde-format
msgid ""
"%1 unauthorized Thunderbolt device was detected. Do you want to authorize it?"
msgid_plural ""
"%1 unauthorized Thunderbolt devices were detected. Do you want to authorize "
"them?"
msgstr[0] ""
"%1 icazəsiz Thunderbolt cihazə aşkar edildi. Buna icazə vermək istəyirsiniz?"
msgstr[1] ""
"%1 icazəsiz Thunderbolt cihazə aşkar edildi. Bunlara icazə vermək "
"istəyirsiniz?"

#: kded_bolt.cpp:76
#, kde-format
msgid "Authorize Now"
msgstr "İndi icazə vermək"

#: kded_bolt.cpp:76
#, kde-format
msgid "Authorize Permanently"
msgstr "Daimi icazə vermək"

#: kded_bolt.cpp:127
#, kde-format
msgid "Thunderbolt Device Authorization Error"
msgstr "Thunderbolt cihazına icazə verilməsində xəta"

#: kded_bolt.cpp:128
#, kde-format
msgid "Failed to authorize Thunderbolt device <b>%1</b>: %2"
msgstr "<b>%1</b> Thunderbolt cihazına icazə verilmədi: %2"
